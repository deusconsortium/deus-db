    {% extends '::layout_admin.html.twig' %}


    {% block page_title %}
        {{ "crud.title.show"|trans([], 'admin') }}
    {% endblock %}

    {% block page_subtitle %}
        {{ "admin.simulation.entity_name"|trans([], "admin") }}
    {% endblock %}

    {% block content %}
    <div class="row">
        <div class="col-md-6">
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">{{ entity }}</h3>
                </div>
                <div class="box-body">
                    <table class="record_properties">
                        <tbody>
                            <tr>
                                <th>{{ "admin.simulation.boxlen"|trans([], "admin") }}</th>
                                    <td>{{ entity.boxlen }}</td>
                            </tr>
                            <tr>
                                <th>{{ "admin.simulation.npart"|trans([], "admin") }}</th>
                                    <td>{{ entity.npart }}</td>
                            </tr>
                            <tr>
                                <th>{{ "admin.simulation.Supercomputer"|trans([], "admin") }}</th>
                                    <td>{{ entity.Supercomputer }}</td>
                            </tr>
                            <tr>
                                <th>{{ "admin.simulation.Project"|trans([], "admin") }}</th>
                                    <td>{{ entity.Project }}</td>
                            </tr>
                            <tr>
                                <th>{{ "admin.simulation.Cosmology"|trans([], "admin") }}</th>
                                    <td>{{ entity.Cosmology }}</td>
                            </tr>                        </tbody>
                    </table>
                </div>
                <div class="box-footer">
                    <a href="{{ path('admin_simulation_edit', { id:entity.id}) }}" class="btn btn-default">{{ 'crud.form.edit'|trans([], 'admin') }}</a>                    <a href="{{ path('admin_simulation_list') }}" class="btn btn-default">{{ 'crud.form.list'|trans([], 'admin') }}</a>
                </div>
                </form>
            </div>
        </div>
    </div>
    {% endblock %}
