{% extends '::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets
        '@data_tables_css'
        '@DeusDBBundle/Resources/public/css/public.css'
        filter="cssrewrite" %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    <script language="JavaScript">
        var myApplication = {
            locale: '{{ app.request.locale }}'
        }
    </script>
    {% javascripts
        '@data_tables'
        '@DeusDBBundle/Resources/public/js/public.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{%  block javascripts_head %}

{% endblock %}

{% block body %}
    {% block content %}
    {% endblock %}
{% endblock %}
